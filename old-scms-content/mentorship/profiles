    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script type="text/javascript">
        //<![CDATA[
		$(function(){
			var qs = getQueryStringObject();

			$('#filterbyforum').change(function(){
			    var forum = $(this).val().toLowerCase();
			    if(forum === 'any')
			        $('#mentor .profile').show();
			    else {
			        $('.profile .areas').each(function () {
			            var ths = $(this);
			            if(ths.html().toLowerCase().indexOf(forum) === -1)
			                ths.parents('.profile').hide();
			            else
			                ths.parents('.profile').show();
			        });
			    }
			});

			if(qs['filter']) {
				$('#filterbyforum').val(decodeURI(qs['filter']));
                                $('#filterbyforum').change();
                        }
		});

		function getQueryStringObject(){
			var loc = location.toString();
			var qsIndex = loc.search(/\?/);
			var queryStringValues = {};

			if(qsIndex > 0)
			{
				var qs = loc.substring(loc.search(/\?/) + 1).split('&');
				var kvtemp;

				for(var i = 0; i < qs.length; i++)
				{
					kvtemp = qs[i].split('=');
					if(kvtemp.length > 1)
						queryStringValues[kvtemp[0]] = kvtemp[1];
				}
			}

			return queryStringValues;
		}
        //]]>
    </script>