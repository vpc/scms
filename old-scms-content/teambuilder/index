			$('.pkmn').each(function() {
				$(this).html(pokemonlist);
			});
			(function(){
			$('.buildpkmn').each(function(i) {
			$($('.buildpkmn')[i]).find('.EV').each( function() { $(this).blur( function() {
				if ($(this).val() < 0 || isNaN($(this).val()) || !$(this).val()) {
					$(this).val('0');
				}
				else if ($(this).val() > 252) {
					$(this).val('252');
				}
				else {
					$(this).val(Math.floor($(this).val()));
				}
			var EVtotal = 0;
			for (var x = 0; x<6; x++) {
				EVtotal += parseInt($($($('.buildpkmn')[i]).find(".EV")[x]).val());
			}
			var EVrem = 508 - EVtotal;
			$($('.buildpkmn')[i]).find('.EVleft').html(EVrem);
			if ($($('.buildpkmn')[i]).find('.EVleft').html() < 0) {
				$($('.buildpkmn')[i]).find('.EVleft').removeClass('positive');
				$($('.buildpkmn')[i]).find('.EVleft').addClass('negative');
			}
			else {
				$($('.buildpkmn')[i]).find('.EVleft').removeClass('negative');
				$($('.buildpkmn')[i]).find('.EVleft').addClass('positive');
			}
			calcStat(i);
			})});
			$($('.buildpkmn')[i]).find('.IV').each( function(){ $(this).blur( function() {
				if($(this).val() < 0 || isNaN($(this).val()) || !$(this).val()) {
					$(this).val('0');
				}
				else if($(this).val() > 31) {
					$(this).val('31');
				}
				else {
					$(this).val(Math.floor($(this).val()));
				}
			calcStat(i);
			})});
			$($('.buildpkmn')[i]).find('.level').each(function() { $(this).blur( function() {
				if ($(this).val() > 100 || isNaN($(this).val()) || !$(this).val()) {
					$(this).val('100');
				}
				else if ($(this).val() < 1) {
					$(this).val('1');
				}
				else {
					$(this).val(Math.floor($(this).val()));
				}
			calcStat(i);
			})});
			$($('.buildpkmn')[i]).find('.nature').each(function() { $(this).change( function () {
				var Natures = {
					Adamant: {plus:'Atk', minus:'SpA'},
					Bashful: {},
					Bold: {plus:'Def', minus:'Atk'},
					Brave: {plus:'Atk', minus:'Spe'},
					Calm: {plus:'SpD', minus:'Atk'},
					Careful: {plus:'SpD', minus:'SpA'},
					Docile: {},
					Gentle: {plus:'SpD', minus:'Def'},
					Hardy: {},
					Hasty: {plus:'Spe', minus:'Def'},
					Impish: {plus:'Def', minus:'SpA'},
					Jolly: {plus:'Spe', minus:'SpA'},
					Lax: {plus:'Def', minus:'SpD'},
					Lonely: {plus:'Atk', minus:'Def'},
					Mild: {plus:'SpA', minus:'Def'},
					Modest: {plus:'SpA', minus:'Atk'},
					Naive: {plus:'Spe', minus:'SpD'},
					Naughty: {plus:'Atk', minus:'SpD'},
					Quiet: {plus:'SpA', minus:'Spe'},
					Quirky: {},
					Rash: {plus:'SpA', minus:'SpD'},
					Relaxed: {plus:'Def', minus:'Spe'},
					Sassy: {plus:'SpD', minus:'Spe'},
					Serious: {},
					Timid: {plus:'Spe', minus:'Atk'}
				};
				$($('.buildpkmn')[i]).find('.stat').each( function() {
					$(this).removeClass('negative');
					$(this).removeClass('positive');
				});
				var nature = $(this).val();
				if (nature && Natures[nature].plus) {
					for (var x=0; x<6; x++) {
						if ($($($('.buildpkmn')[i]).find('.stat')[x]).html() === Natures[nature].plus) {
							$($($('.buildpkmn')[i]).find('.stat')[x]).addClass('positive');
						}
						if ($($($('.buildpkmn')[i]).find('.stat')[x]).html() === Natures[nature].minus) {
							$($($('.buildpkmn')[i]).find('.stat')[x]).addClass('negative');
						}					
					}
				}
			calcStat(i);
			})});
			$($('.buildpkmn')[i]).find('.move').each(function(x) { $(this).change( function () {
				changeMoves(i,x);
			})});
			if ($(this).find('a[href$="/pokemon/"]').length === 1) {
				$(this).find('.dexbutton > a').text('Go to PokÃ©dex!');
			}
			else {
				$(this).find('.dexbutton > a').text('Go to Analysis!');
			}
			$('input[name=sim]').each(function() { $(this).change( function () {
				exportteam();
			})});
			})})();
		});
		function choosemon(pkmnno) {
			var TierArray = ["Doubles","Little Cup","Neverused","Overused","Rarelyused","Uber","Underused","VGC &#39;11","VGC &#39;12"];
			var TierArray2 = ["doubles","lc","nu","ou","ru","uber","uu","vgc2011","vgc2012"];
			var TierChosen = $($('.buildpkmn')[pkmnno]).find('.tier').val();
			var tier = '';
			var PokemonChosen = $('.pkmn option:selected').eq(pkmnno).val();
			PokemonChosen = PokemonChosen.toLowerCase();
			if (PokemonChosen === "farfetch'd") {
				PokemonChosen = 'farfetchd';
			}
			else if (PokemonChosen === "mr. mime") {
				PokemonChosen = "mr_mime";
			}
			else if (PokemonChosen === "mime jr.") {
				PokemonChosen = "mime_jr";
			}
			if ($('.pkmn')[pkmnno].selectedIndex !== 0) {
			for (var i=0; i<TierArray.length; i++) {
				var tierrg = new RegExp(TierArray[i]);
				if (tierrg.test(TierChosen) === true) {
					tier = TierArray2[i];
				}
			}
			$.get("/bw/pokemon/" + PokemonChosen, function(data) {
				step1(data, pkmnno);
			});
			$($('.dexbutton > a')[pkmnno]).attr("href", "/bw/pokemon/" + PokemonChosen);
			}
			else {
			$($('.tier')[pkmnno]).html('');
			$($('.ability')[pkmnno]).html('');
			$($('.item')[pkmnno]).html('');
			$($('.nature')[pkmnno]).html('');
			$($('.set')[pkmnno]).html('');
			$($('.level')[pkmnno]).val('');
			$($('.EVleft')[pkmnno]).html('508');
			$($('.EVleft')[pkmnno]).removeClass('negative');
			$($('.EVleft')[pkmnno]).removeClass('positive');
			$($('.dexbutton > a')[pkmnno]).attr("href", "/bw/pokemon/");
			$('.buildpkmn').eq(pkmnno).find('.EV').each(function() {$(this).val('0');});
			$('.buildpkmn').eq(pkmnno).find('.IV').each(function() {$(this).val('31');});
			$('.buildpkmn').eq(pkmnno).find('.move').each(function() {$(this).html('');});
			$('.buildpkmn').eq(pkmnno).find('.base').each(function() {$(this).val('0');});
			$('.buildpkmn').eq(pkmnno).find('.pstat').each(function() {$(this).val('0');});
			$('.buildpkmn').eq(pkmnno).find('.spriteholder').html('');
			setNature(pkmnno);
			}
		}
		function setNature(i) {
			var Natures = {
				Adamant: {plus:'Atk', minus:'SpA'},
				Bashful: {},
				Bold: {plus:'Def', minus:'Atk'},
				Brave: {plus:'Atk', minus:'Spe'},
				Calm: {plus:'SpD', minus:'Atk'},
				Careful: {plus:'SpD', minus:'SpA'},
				Docile: {},
				Gentle: {plus:'SpD', minus:'Def'},
				Hardy: {},
				Hasty: {plus:'Spe', minus:'Def'},
				Impish: {plus:'Def', minus:'SpA'},
				Jolly: {plus:'Spe', minus:'SpA'},
				Lax: {plus:'Def', minus:'SpD'},
				Lonely: {plus:'Atk', minus:'Def'},
				Mild: {plus:'SpA', minus:'Def'},
				Modest: {plus:'SpA', minus:'Atk'},
				Naive: {plus:'Spe', minus:'SpD'},
				Naughty: {plus:'Atk', minus:'SpD'},
				Quiet: {plus:'SpA', minus:'Spe'},
				Quirky: {},
				Rash: {plus:'SpA', minus:'SpD'},
				Relaxed: {plus:'Def', minus:'Spe'},
				Sassy: {plus:'SpD', minus:'Spe'},
				Serious: {},
				Timid: {plus:'Spe', minus:'Atk'}
			};
			$($('.buildpkmn')[i]).find('.stat').each( function() {
				$(this).removeClass('negative');
				$(this).removeClass('positive');
			});
			var nature = $($('.buildpkmn')[i]).find('.nature').val();
			if (nature && Natures[nature].plus) {
				for (var x=0; x<6; x++) {
					if ($($($('.buildpkmn')[i]).find('.stat')[x]).html() === Natures[nature].plus) {
						$($($('.buildpkmn')[i]).find('.stat')[x]).addClass('positive');
					}
					if ($($($('.buildpkmn')[i]).find('.stat')[x]).html() === Natures[nature].minus) {
						$($($('.buildpkmn')[i]).find('.stat')[x]).addClass('negative');
					}					
				}
			}
			calcStat(i);
		}
		function choosetier(pkmnno) {
			var TierArray = ["Doubles","Little Cup","Neverused","Overused","Rarelyused","Uber","Underused","VGC '11","VGC '12"];
			var TierArray2 = ["doubles","lc","nu","ou","ru","uber","uu","vgc2011","vgc2012"]
			var TierChosen = $($('.buildpkmn')[pkmnno]).find('.tier').val();
			var tier = '';
			var PokemonChosen = $('.pkmn option:selected').eq(pkmnno).val();
			PokemonChosen = PokemonChosen.toLowerCase();	
			if (PokemonChosen === "farfetch'd") {
				PokemonChosen = 'farfetchd';
			}
			else if (PokemonChosen === "mr. mime") {
				PokemonChosen = "mr_mime";
			}
			else if (PokemonChosen === "mime jr.") {
				PokemonChosen = "mime_jr";
			}			
			for (var i=0; i<TierArray.length; i++) {
				var tierrg = new RegExp(TierArray[i]);
				if (tierrg.test(TierChosen) === true) {
					tier = TierArray2[i];
				}
			}
			if (tier) {
				tier = "/" + tier;
			}
			$.get("/bw/pokemon/" + PokemonChosen + tier, function(data) {
				step1(data, pkmnno);
			});
			$($('.dexbutton > a')[pkmnno]).attr("href", "/bw/pokemon/" + PokemonChosen + tier);
		}
		function calcStat(i) {
			var baseArray = [];
			var IVArray = [];
			var EVArray = [];
			for (var x=0; x<6; x++) {
				baseArray.push(parseInt($($($('.buildpkmn')[i]).find('.base')[x]).val()));
				IVArray.push(parseInt($($($('.buildpkmn')[i]).find('.IV')[x]).val()));
				EVArray.push(parseInt($($($('.buildpkmn')[i]).find('.EV')[x]).val()));
			}
			var Level = parseInt($($('.buildpkmn')[i]).find('.level').val());
			var HPstat;
			if (baseArray[0] !== 1 && baseArray[0] !==0) {
				HPstat = Math.floor(Math.floor(2*baseArray[0] + IVArray[0] + Math.floor(EVArray[0]/4)) * Level / 100 + Level + 10);
			}
			else if (baseArray[0] === 1) {
				HPstat = 1;
			}
			else {
				//do nothing
			}
			if (HPstat) {
				$($($('.buildpkmn')[i]).find('.pstat')[0]).val(HPstat);
			}
			for (var s=1; s<6; s++) {
				var Nature;
				if ($($($('.buildpkmn')[i]).find('.stat')[s]).hasClass('positive')) {
					Nature = 1.1;
				}
				else if ($($($('.buildpkmn')[i]).find('.stat')[s]).hasClass('negative')) {
					Nature = 0.9
				}
				else {
					Nature = 1.0;
				}
				if (baseArray[s] !== 0){
					var finstat = Math.floor(Math.floor(Math.floor(2*baseArray[s] + IVArray[s] + Math.floor(EVArray[s]/4)) * Level / 100 + 5) * Nature)
					$($($('.buildpkmn')[i]).find('.pstat')[s]).val(finstat);
				}
			}
			HiddenPowah(i);
		}
		function HiddenPowah(i) {
			var IVArray = [];
			for (var x=0; x<6; x++) {
				IVArray.push(parseInt($($($('.buildpkmn')[i]).find('.IV')[x]).val()));
			}		
			var TypeArray = [1,2,4,16,32,8];
			for (var y=0; y<6; y++) {
				if (IVArray[y]%2 === 0) {
					TypeArray[y] = 0;
				}
			}
			var TypeSum = 0;
			for (var z=0; z<6; z++) {
				TypeSum += TypeArray[z];
			}
			var TypeNum = Math.floor(TypeSum*15/63);
			var Type = ["Fighting", "Flying", "Poison", "Ground", "Rock", "Bug", "Ghost", "Steel", "Fire", "Water", "Grass", "Electric", "Psychic", "Ice", "Dragon", "Dark"];
			var PowerArray = [1,2,4,16,32,8];
			for (var a=0; a<6; a++) {
				if(IVArray[a]%4 === 0 || IVArray[a]%4 === 1) {
					PowerArray[a] = 0;
				}
			}
			var PowerSum = 0;
			for (var b=0; b<6; b++) {
				PowerSum += PowerArray[b];
			}
			var Power = Math.floor(PowerSum*(40/63) + 30);
			$($('.buildpkmn')[i]).find('.HPtype').val(Type[TypeNum]);
			$($('.buildpkmn')[i]).find('.HPbp').val(Power);
		}
		function setHiddenPower(i) {
			//this function is only activated when the moveset does not specify IVs	
			var hasHiddenPower = false;
			var HiddenPowerIVs = {
				Bug: [31,30,30,31,30,31],
				Dark: [31,31,31,31,31,31],
				Dragon: [31,30,31,31,31,31],
				Flying: [30,30,30,30,30,31],
				Ghost: [31,31,30,31,30,31],
				Ground: [31,31,31,30,30,31],
				Poison: [31,31,31,30,30,31],
				Psychic: [31,30,31,31,31,30],
				Rock: [31,31,30,31,30,30],
				Steel: [31,31,31,31,30,31],
				Water: [31,30,30,30,31,31],
				Electric: [31,31,31,30,31,31],
				Fighting: [31,31,30,30,30,30],
				Fire: [31,30,31,30,31,30],
				Grass: [31,30,31,30,31,31],
				Ice: [31,30,30,31,31,31]
			};
			for (var a=0; a<4; a++) {
				if ($($($('.buildpkmn')[i]).find('.move')[a]).val().substr(0,13) === 'Hidden Power ') {
					hasHiddenPower = true;
					var type = $($($('.buildpkmn')[i]).find('.move')[a]).val().substr(13);
				}
			}
			if (hasHiddenPower === true) {
				for (var b=0; b<6; b++) {
					$($($('.buildpkmn')[i]).find('.IV')[b]).val(HiddenPowerIVs[type][b]);
				}
			}
		}
		function changeMoves(i,x) {
			var hasHiddenPower = false;
			var isSlow = false;
			var HiddenPowerIVs = {
				Bug: [31,30,30,31,30,31],
				Dark: [31,31,31,31,31,31],
				Dragon: [31,30,31,31,31,31],
				Flying: [30,30,30,30,30,31],
				Ghost: [31,31,30,31,30,31],
				Ground: [31,31,31,30,30,31],
				Poison: [31,31,31,30,30,31],
				Psychic: [31,30,31,31,31,30],
				Rock: [31,31,30,31,30,30],
				Steel: [31,31,31,31,30,31],
				Water: [31,30,30,30,31,31],
				Electric: [31,31,31,30,31,31],
				Fighting: [31,31,30,30,30,30],
				Fire: [31,30,31,30,31,30],
				Grass: [31,30,31,30,31,31],
				Ice: [31,30,30,31,31,31]
			};
			//checks if Pokemon has Trick Room, Gyro Ball, or Hidden Power
			for (var b=0; b<4; b++) {
				if ($($($('.buildpkmn')[i]).find('.move')[b]).val() === 'Trick Room' || $($($('.buildpkmn')[i]).find('.move')[b]).val() === 'Gyro Ball') {
					isSlow = true;
				}
				if ($($($('.buildpkmn')[i]).find('.move')[b]).val().substr(0,13) === 'Hidden Power ') {
					hasHiddenPower = true;
				}
			}
			//if move changed to Hidden Power, lower Speed IV according to whether Pokemon has Gyro Ball / Trick Room
			if ($($($('.buildpkmn')[i]).find('.move')[x]).val().substr(0,13) === 'Hidden Power ') {
				hasHiddenPower = true;
				var type = $($($('.buildpkmn')[i]).find('.move')[x]).val().substr(13);
				for (var a=0; a<6; a++) {
					$($($('.buildpkmn')[i]).find('.IV')[a]).val(HiddenPowerIVs[type][a]);
				}
				if (isSlow) {
					$($($('.buildpkmn')[i]).find('.IV')[5]).val(parseInt($($($('.buildpkmn')[i]).find('.IV')[5]).val())%4);
				}
			}
			//if move changed to Trick Room or Gyro Ball, lower Speed IV according to whether Pokemon has Hidden Power
			if ($($($('.buildpkmn')[i]).find('.move')[x]).val() === 'Trick Room' || $($($('.buildpkmn')[i]).find('.move')[x]).val() === 'Gyro Ball') {
				if (!hasHiddenPower) {
					$($($('.buildpkmn')[i]).find('.IV')[5]).val('0');
				}
				else {
					$($($('.buildpkmn')[i]).find('.IV')[5]).val(parseInt($($($('.buildpkmn')[i]).find('.IV')[5]).val())%4);
				}
			}
			HiddenPowah(i);
			calcStat(i);
		}
		function step1(x, pkmnno) {
			var spritehtml = $(x).find('.sprite');
			$('.buildpkmn').eq(pkmnno).find('.spriteholder').html(spritehtml);
			var dexset = $(x).find('.info.strategyheader').find('h2');
			var setlist = '<option selected></option>';
			for (var i=0; i<dexset.length; i++) {
				setlist += '<option>'+ $(dexset[i]).html() + '</option>';
			}
			$($('.set')[pkmnno]).html(setlist);
			var tierlist = '';
			var montabs = $(x).find('.tabs');
			var correcttab = montabs.length-1;
			var montier = $(montabs[correcttab]).find('li');
			tierlist = '<option>' + $(montabs[correcttab]).find('strong').html() + '</option>';
			for (var a=0; a<montier.length; a++) {
				if ($(montier[a]).find('a').html()) {
				tierlist += '<option>' + $(montier[a]).find('a').html() + '</option>';		
				}
			}
			var AllAbilities = [];
			var AbilityTD = $(x).find('.ability').find('dt');
			for (var j=0; j<AbilityTD.length; j++) {
				AllAbilities.push($(AbilityTD[j]).find('a').html());
			}
			var abilitylist = '<option></option>';
			for (var q=0; q<AllAbilities.length; q++) {
				abilitylist += '<option>' + AllAbilities[q] + '</option>';
			}
			var MovesArray = ["Absorb","Acid","Acid Armor","Acid Spray","Acrobatics","Acupressure","Aerial Ace","Aeroblast","After You","Agility","Air Cutter","Air Slash","Ally Switch","Amnesia","AncientPower","Aqua Jet","Aqua Ring","Aqua Tail","Arm Thrust","Aromatherapy","Assist","Assurance","Astonish","Attack Order","Attract","Aura Sphere","Aurora Beam","Autotomize","Avalanche","Barrage","Barrier","Baton Pass","Beat Up","Belly Drum","Bestow","Bide","Bind","Bite","Blast Burn","Blaze Kick","Blizzard","Block","Blue Flare","Body Slam","Bolt Strike","Bone Club","Bone Rush","Bonemerang","Bounce","Brave Bird","Brick Break","Brine","Bubble","BubbleBeam","Bug Bite","Bug Buzz","Bulk Up","Bulldoze","Bullet Punch","Bullet Seed","Calm Mind","Camouflage","Captivate","Charge","Charge Beam","Charm","Chatter","Chip Away","Circle Throw","Clamp","Clear Smog","Close Combat","Coil","Comet Punch","Confuse Ray","Confusion","Constrict","Conversion","Conversion 2","Copycat","Cosmic Power","Cotton Guard","Cotton Spore","Counter","Covet","Crabhammer","Cross Chop","Cross Poison","Crunch","Crush Claw","Crush Grip","Curse","Cut","Dark Pulse","Dark Void","Defend Order","Defense Curl","Defog","Destiny Bond","Detect","Dig","Disable","Discharge","Dive","Dizzy Punch","Doom Desire","Double-Edge","Double Hit","Double Kick","DoubleSlap","Double Team","Draco Meteor","DragonBreath","Dragon Claw","Dragon Dance","Dragon Pulse","Dragon Rage","Dragon Rush","Dragon Tail","Drain Punch","Dream Eater","Drill Peck","Drill Run","Dual Chop","DynamicPunch","Earth Power","Earthquake","Echoed Voice","Egg Bomb","Electro Ball","Electroweb","Embargo","Ember","Encore","Endeavor","Endure","Energy Ball","Entrainment","Eruption","Explosion","Extrasensory","ExtremeSpeed","Facade","Faint Attack","Fake Out","Fake Tears","False Swipe","FeatherDance","Feint","Fiery Dance","Final Gambit","Fire Blast","Fire Fang","Fire Pledge","Fire Punch","Fire Spin","Fissure","Flail","Flame Burst","Flame Charge","Flame Wheel","Flamethrower","Flare Blitz","Flash","Flash Cannon","Flatter","Fling","Fly","Focus Blast","Focus Energy","Focus Punch","Follow Me","Force Palm","Foresight","Foul Play","Freeze Shock","Frenzy Plant","Frost Breath","Frustration","Fury Attack","Fury Cutter","Fury Swipes","Fusion Bolt","Fusion Flare","Future Sight","Gastro Acid","Gear Grind","Giga Drain","Giga Impact","Glaciate","Glare","Grass Knot","Grass Pledge","GrassWhistle","Gravity","Growl","Growth","Grudge","Guard Split","Guard Swap","Guillotine","Gunk Shot","Gust","Gyro Ball","Hail","Hammer Arm","Harden","Haze","Head Charge","Head Smash","Headbutt","Heal Bell","Heal Block","Heal Order","Heal Pulse","Healing Wish","Heart Stamp","Heart Swap","Heat Crash","Heat Wave","Heavy Slam","Helping Hand","Hex","Hi Jump Kick","Hidden Power","Hidden Power Bug","Hidden Power Dark","Hidden Power Dragon","Hidden Power Electric","Hidden Power Fighting","Hidden Power Fire","Hidden Power Flying","Hidden Power Ghost","Hidden Power Grass","Hidden Power Ground","Hidden Power Ice","Hidden Power Poison","Hidden Power Psychic","Hidden Power Rock","Hidden Power Steel","Hidden Power Water","Hone Claws","Horn Attack","Horn Drill","Horn Leech","Howl","Hurricane","Hydro Cannon","Hydro Pump","Hyper Beam","Hyper Fang","Hyper Voice","Hypnosis","Ice Ball","Ice Beam","Ice Burn","Ice Fang","Ice Punch","Ice Shard","Icicle Crash","Icicle Spear","Icy Wind","Imprison","Incinerate","Inferno","Ingrain","Iron Defense","Iron Head","Iron Tail","Judgment","Jump Kick","Karate Chop","Kinesis","Knock Off","Last Resort","Lava Plume","Leaf Blade","Leaf Storm","Leaf Tornado","Leech Life","Leech Seed","Leer","Lick","Light Screen","Lock-On","Lovely Kiss","Low Kick","Low Sweep","Lucky Chant","Lunar Dance","Luster Purge","Mach Punch","Magic Coat","Magic Room","Magical Leaf","Magma Storm","Magnet Bomb","Magnet Rise","Magnitude","Me First","Mean Look","Meditate","Mega Drain","Mega Kick","Mega Punch","Megahorn","Memento","Metal Burst","Metal Claw","Metal Sound","Meteor Mash","Metronome","Milk Drink","Mimic","Mind Reader","Minimize","Miracle Eye","Mirror Coat","Mirror Move","Mirror Shot","Mist","Mist Ball","Moonlight","Morning Sun","Mud-Slap","Mud Bomb","Mud Shot","Mud Sport","Muddy Water","Nasty Plot","Natural Gift","Nature Power","Needle Arm","Night Daze","Night Shade","Night Slash","Nightmare","Octazooka","Odor Sleuth","Ominous Wind","Outrage","Overheat","Pain Split","Pay Day","Payback","Peck","Perish Song","Petal Dance","Pin Missile","Pluck","Poison Fang","Poison Gas","Poison Jab","PoisonPowder","Poison Sting","Poison Tail","Pound","Powder Snow","Power Gem","Power Split","Power Swap","Power Trick","Power Whip","Present","Protect","Psybeam","Psych Up","Psychic","Psycho Boost","Psycho Cut","Psycho Shift","Psyshock","Psystrike","Psywave","Punishment","Pursuit","Quash","Quick Attack","Quick Guard","Quiver Dance","Rage","Rage Powder","Rain Dance","Rapid Spin","Razor Leaf","Razor Shell","Razor Wind","Recover","Recycle","Reflect","Reflect Type","Refresh","Relic Song","Rest","Retaliate","Return","Revenge","Reversal","Roar","Roar of Time","Rock Blast","Rock Climb","Rock Polish","Rock Slide","Rock Smash","Rock Throw","Rock Tomb","Rock Wrecker","Role Play","Rolling Kick","Rollout","Roost","Round","Sacred Fire","Sacred Sword","Safeguard","Sand-Attack","Sand Tomb","Sandstorm","Scald","Scary Face","Scratch","Screech","Searing Shot","Secret Power","Secret Sword","Seed Bomb","Seed Flare","Seismic Toss","Selfdestruct","Shadow Ball","Shadow Claw","Shadow Force","Shadow Punch","Shadow Sneak","Sharpen","Sheer Cold","Shell Smash","Shift Gear","Shock Wave","Signal Beam","Silver Wind","Simple Beam","Sing","Sketch","Skill Swap","Skull Bash","Sky Attack","Sky Drop","Sky Uppercut","Slack Off","Slam","Slash","Sleep Powder","Sleep Talk","Sludge","Sludge Bomb","Sludge Wave","Smack Down","SmellingSalt","Smog","SmokeScreen","Snarl","Snatch","Snore","Soak","Softboiled","SolarBeam","SonicBoom","Spacial Rend","Spark","Spider Web","Spike Cannon","Spikes","Spit Up","Spite","Splash","Spore","S