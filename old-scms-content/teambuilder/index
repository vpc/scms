			var movelist = '<option></option>'; 
			for (var b=0; b<MovesArray.length; b++) {
				movelist += '<option>' + MovesArray[b] + '</option>'
			}
			var ItemArray = ["Adamant Orb", "Air Balloon", "Babiri Berry", "Black Belt", "Black Sludge", "BlackGlasses", "BrightPowder", "Bug Gem", "Burn Drive", "Charcoal", "Charti Berry", "Chesto Berry", "Chilan Berry", "Chill Drive", "Choice Band", "Choice Scarf", "Choice Specs", "Chople Berry", "Coba Berry", "Colbur Berry", "Custap Berry", "Damp Rock", "Dark Gem", "Douse Drive", "Draco Plate", "Dragon Fang", "Dragon Gem", "Dread Plate", "Earth Plate", "Eject Button", "Electric Gem", "Eviolite", "Expert Belt", "Fighting Gem", "Fire Gem", "Fist Plate", "Flame Orb", "Flame Plate", "Flying Gem", "Focus Sash", "Ghost Gem", "Grass Gem", "Grip Claw", "Griseous Orb", "Ground Gem", "Haban Berry", "Hard Stone", "Heat Rock", "Ice Gem", "Icicle Plate", "Icy Rock", "Insect Plate", "Iron Ball", "Iron Plate", "Kasib Berry", "Kebia Berry", "Kings Rock", "Lagging Tail", "Leftovers", "Leppa Berry", "Liechi Berry", "Life Orb", "Light Ball", "Light Clay", "Lum Berry", "Lustrous Orb", "Magnet", "Mail", "Meadow Plate", "Metal Coat", "Metal Powder", "Mind Plate", "Miracle Seed", "Mystic Water", "Nevermeltice", "Normal Gem", "Occa Berry", "Oran Berry", "Passho Berry", "Payapa Berry", "Persim Berry", "Petaya Berry", "Poison Barb", "Poison Gem", "Psychic Gem", "Quick Claw", "Quick Powder", "Razor Claw", "Razor Fang", "Rindo Berry", "Ring Target", "Rock Gem", "Rocky Helmet", "Salac Berry", "Scope Lens", "Sharp Beak", "Shuca Berry", "Silk Scarf", "SilverPowder", "Sitrus Berry", "Sky Plate", "Smooth Rock", "Soft Sand", "Splash Plate", "Spooky Plate", "Steel Gem", "Stick", "Stone Plate", "Tanga Berry", "Thick Club", "Toxic Orb", "Toxic Plate", "TwistedSpoon", "Wacan Berry", "Water Gem", "White Herb", "Wide Lens", "Wise Glasses", "Yache Berry", "Zap Plate", "Zoom Lens"];
			var itemlist = '<option></option>';
			for (var f=0; f<ItemArray.length; f++) {
				itemlist += '<option>' + ItemArray[f] + '</option>';
			}
			var NatureArray = ["Adamant", "Bashful", "Bold", "Brave", "Calm", "Careful", "Docile", "Gentle", "Hardy", "Hasty", "Impish", "Jolly", "Lax", "Lonely", "Mild", "Modest", "Naive", "Naughty", "Quiet", "Quirky", "Rash", "Relaxed", "Sassy", "Serious", "Timid"];
			var naturelist = '<option></option>';
			for (var n=0; n<NatureArray.length; n++) {
				naturelist += '<option>' + NatureArray[n] + '</option>';
			}
			var tier = $(montabs[correcttab]).find('strong').html();
			var level = '';
			if (tier === 'Little Cup') {
				level = '5';
			}
			else if (tier === "VGC '12" || tier === "VGC '13") {
				level = '50';
			}
			else {
				level = '100';
			}
			var stats = $(x).find('.bar div');
			for (var c=0; c<stats.length; c++) {
				$($($('.buildpkmn')[pkmnno]).find('.base')[c]).val($($(stats)[c]).html());
			}
			$($('.tier')[pkmnno]).html(tierlist);
			$($('.ability')[pkmnno]).html(abilitylist);
			$($('.item')[pkmnno]).html(itemlist);
			$($('.nature')[pkmnno]).html(naturelist);
			$($('.level')[pkmnno]).val(level);
			$($('.EVleft')[pkmnno]).html('508');
			$($('.EVleft')[pkmnno]).removeClass('negative');
			$($('.EVleft')[pkmnno]).removeClass('positive');
			$('.buildpkmn').eq(pkmnno).find('.EV').each(function() {$(this).val('0');});
			$('.buildpkmn').eq(pkmnno).find('.IV').each(function() {$(this).val('31');});
			$('.buildpkmn').eq(pkmnno).find('.move').each(function() {$(this).html(movelist);});
			setNature(pkmnno);
		}
		function findset(g) {	
			var setno = $('.set')[g].selectedIndex-1;
			var PokemonChosen = $('.pkmn option:selected').eq(g).val();
			PokemonChosen = PokemonChosen.toLowerCase();
			if (PokemonChosen === "farfetch'd") {
				PokemonChosen = 'farfetchd';
			}
			else if (PokemonChosen === "mr. mime") {
				PokemonChosen = "mr_mime";
			}
			else if (PokemonChosen === "mime jr.") {
				PokemonChosen = "mime_jr";
			}			
			var TierArray = ["Doubles","Little Cup","Neverused","Overused","Rarelyused","Uber","Underused","VGC '11","VGC '12"];
			var TierArray2 = ["doubles","lc","nu","ou","ru","uber","uu","vgc2011","vgc2012"];
			var TierChosen = $($('.buildpkmn')[g]).find('.tier').val();
			var tier = '';
			for (var v=0; v<TierArray.length; v++) {
				var tierrg = new RegExp(TierArray[v]);
				if (tierrg.test(TierChosen) === true) {
					tier = TierArray2[v];
				}
			}
			if (tier) {
			tier = "/" + tier;
			}
			var setname = $($('.set')[g]).val();
			setname = setname.replace(/\s/g,"_");
			setname = '#' + setname;
			$($('.dexbutton > a')[g]).attr("href", "/bw/pokemon/" + PokemonChosen + tier + setname);
			$.get("/bw/pokemon/" + PokemonChosen + tier, function(data) {
				var r = $(data).find('.info.strategyheader');
				var hasability = $(r[setno]).find('td').length;
			if (hasability === 3) { 
				var ability = $(data).find('.ability').find('dt');
				var abilitylist;
				for (var d=0; d<ability.length; d++) {
					abilitylist += '<option>' + $(ability[d]).find('a').html() + '</option>';
				}
			}
			else {
				var ability = $(r[setno]).find('td')[2];
				var ability2 = $(ability).find('a');
				var abilitylist;
				var setabilities = [];
				for (var c=0; c<ability2.length; c++) {
					abilitylist += '<option>'+ $(ability2[c]).html() + '</option>';
					setabilities.push($(ability2[c]).html());
				}
				var AllAbilities = [];
				var AbilityTD = $(data).find('.ability').find('dt');
				for (var j=0; j<AbilityTD.length; j++) {
					AllAbilities.push($(AbilityTD[j]).find('a').html());
				}
				AllAbilities = $.grep(AllAbilities, function(n, i){
					return $.inArray(n, setabilities) === -1;
				});
				if (AllAbilities) {
					abilitylist += '<option></option>';
					for (var a=0; a<AllAbilities.length; a++) {
						abilitylist += '<option>' + AllAbilities[a] + '</option>'
					}
				}
			}
				var item = $(r[setno]).find('td')[1];
				var item2 = $(item).find('a');
				var setitems = [];
				var itemlist;
				var ItemArray = ["Adamant Orb", "Air Balloon", "Babiri Berry", "Black Belt", "Black Sludge", "BlackGlasses", "BrightPowder", "Bug Gem", "Burn Drive", "Charcoal", "Charti Berry", "Chesto Berry", "Chilan Berry", "Chill Drive", "Choice Band", "Choice Scarf", "Choice Specs", "Chople Berry", "Coba Berry", "Colbur Berry", "Custap Berry", "Damp Rock", "Dark Gem", "Douse Drive", "Draco Plate", "Dragon Fang", "Dragon Gem", "Dread Plate", "Earth Plate", "Eject Button", "Electric Gem", "Eviolite", "Expert Belt", "Fighting Gem", "Fire Gem", "Fist Plate", "Flame Orb", "Flame Plate", "Flying Gem", "Focus Sash", "Ghost Gem", "Grass Gem", "Grip Claw", "Griseous Orb", "Ground Gem", "Haban Berry", "Hard Stone", "Heat Rock", "Ice Gem", "Icicle Plate", "Icy Rock", "Insect Plate", "Iron Ball", "Iron Plate", "Kasib Berry", "Kebia Berry", "Kings Rock", "Lagging Tail", "Leftovers", "Leppa Berry", "Liechi Berry", "Life Orb", "Light Ball", "Light Clay", "Lum Berry", "Lustrous Orb", "Magnet", "Meadow Plate", "Metal Coat", "Metal Powder", "Mind Plate", "Miracle Seed", "Mystic Water", "Nevermeltice", "Normal Gem", "Occa Berry", "Oran Berry", "Passho Berry", "Payapa Berry", "Persim Berry", "Petaya Berry", "Poison Barb", "Poison Gem", "Psychic Gem", "Quick Claw", "Quick Powder", "Razor Claw", "Razor Fang", "Rindo Berry", "Ring Target", "Rock Gem", "Rocky Helmet", "Salac Berry", "Scope Lens", "Sharp Beak", "Shuca Berry", "Silk Scarf", "SilverPowder", "Sitrus Berry", "Sky Plate", "Smooth Rock", "Soft Sand", "Splash Plate", "Spooky Plate", "Steel Gem", "Stick", "Stone Plate", "Tanga Berry", "Thick Club", "Toxic Orb", "Toxic Plate", "TwistedSpoon", "Wacan Berry", "Water Gem", "White Herb", "Wide Lens", "Wise Glasses", "Yache Berry", "Zap Plate", "Zoom Lens"];
				for (var f=0; f<item2.length; f++) {
					itemlist += '<option>' + $(item2[f]).html() + '</option>';
					setitems.push($(item2[f]).html());
				}
				ItemArray = $.grep(ItemArray, function(n, i){
					return $.inArray(n, setitems) === -1;
				});
				itemlist += '<option></option>';
				for (var z=0; z<ItemArray.length; z++) {
					itemlist += '<option>' + ItemArray[z] + '</option>';
				}
				var findnature = $(r[setno]).find('td').length-1;
				var nature = $(r[setno]).find('td')[findnature];
				var nature2 = $(nature).find('a');
				var naturelist;
				var setnature = [];
				for (var n=0; n<nature2.length; n++) {
					naturelist += '<option>' + $(nature2[n]).html() + '</option>';
					setnature.push($(nature2[n]).html());
				}
				var NatureArray = ["Adamant", "Bashful", "Bold", "Brave", "Calm", "Careful", "Docile", "Gentle", "Hardy", "Hasty", "Impish", "Jolly", "Lax", "Lonely", "Mild", "Modest", "Naive", "Naughty", "Quiet", "Quirky", "Rash", "Relaxed", "Sassy", "Serious", "Timid"];
				NatureArray = $.grep(NatureArray, function(n, i){
					return $.inArray(n, setnature) === -1;
				});				
				naturelist += '<option></option>';
				for (var t=0; t<NatureArray.length; t++) {
					naturelist += '<option>' + NatureArray[t] + '</option>';
				}
				var moves = $(data).find('.info.moveset');
				var moveset = $(moves[setno]).find('td')[0];
				var moveset2 = $(moveset).html();
				var movearray = moveset2.split('~');
				var movelist;
				for (var i=1; i<movearray.length; i++) {
					var tempmove = movearray[i].split(/\n\/\n/g);
					function movefunction(tempmove, g, i){
						var setmoves = [];
						for (var j=0; j<tempmove.length; j++) {
							movelist += '<option>' + $(tempmove[j]).html() + '</option>';
							setmoves.push($(tempmove[j]).html());
						}
						movelist += '<option></option>';
						var MovesArray = ["Absorb","Acid","Acid Armor","Acid Spray","Acrobatics","Acupressure","Aerial Ace","Aeroblast","After You","Agility","Air Cutter","Air Slash","Ally Switch","Amnesia","AncientPower","Aqua Jet","Aqua Ring","Aqua Tail","Arm Thrust","Aromatherapy","Assist","Assurance","Astonish","Attack Order","Attract","Aura Sphere","Aurora Beam","Autotomize","Avalanche","Barrage","Barrier","Baton Pass","Beat Up","Belly Drum","Bestow","Bide","Bind","Bite","Blast Burn","Blaze Kick","Blizzard","Block","Blue Flare","Body Slam","Bolt Strike","Bone Club","Bone Rush","Bonemerang","Bounce","Brave Bird","Brick Break","Brine","Bubble","BubbleBeam","Bug Bite","Bug Buzz","Bulk Up","Bulldoze","Bullet Punch","Bullet Seed","Calm Mind","Camouflage","Captivate","Charge","Charge Beam","Charm","Chatter","Chip Away","Circle Throw","Clamp","Clear Smog","Close Combat","Coil","Comet Punch","Confuse Ray","Confusion","Constrict","Conversion","Conversion 2","Copycat","Cosmic Power","Cotton Guard","Cotton Spore","Counter","Covet","Crabhammer","Cross Chop","Cross Poison","Crunch","Crush Claw","Crush Grip","Curse","Cut","Dark Pulse","Dark Void","Defend Order","Defense Curl","Defog","Destiny Bond","Detect","Dig","Disable","Discharge","Dive","Dizzy Punch","Doom Desire","Double-Edge","Double Hit","Double Kick","DoubleSlap","Double Team","Draco Meteor","DragonBreath","Dragon Claw","Dragon Dance","Dragon Pulse","Dragon Rage","Dragon Rush","Dragon Tail","Drain Punch","Dream Eater","Drill Peck","Drill Run","Dual Chop","DynamicPunch","Earth Power","Earthquake","Echoed Voice","Egg Bomb","Electro Ball","Electroweb","Embargo","Ember","Encore","Endeavor","Endure","Energy Ball","Entrainment","Eruption","Explosion","Extrasensory","ExtremeSpeed","Facade","Faint Attack","Fake Out","Fake Tears","False Swipe","FeatherDance","Feint","Fiery Dance","Final Gambit","Fire Blast","Fire Fang","Fire Pledge","Fire Punch","Fire Spin","Fissure","Flail","Flame Burst","Flame Charge","Flame Wheel","Flamethrower","Flare Blitz","Flash","Flash Cannon","Flatter","Fling","Fly","Focus Blast","Focus Energy","Focus Punch","Follow Me","Force Palm","Foresight","Foul Play","Freeze Shock","Frenzy Plant","Frost Breath","Frustration","Fury Attack","Fury Cutter","Fury Swipes","Fusion Bolt","Fusion Flare","Future Sight","Gastro Acid","Gear Grind","Giga Drain","Giga Impact","Glaciate","Glare","Grass Knot","Grass Pledge","GrassWhistle","Gravity","Growl","Growth","Grudge","Guard Split","Guard Swap","Guillotine","Gunk Shot","Gust","Gyro Ball","Hail","Hammer Arm","Harden","Haze","Head Charge","Head Smash","Headbutt","Heal Bell","Heal Block","Heal Order","Heal Pulse","Healing Wish","Heart Stamp","Heart Swap","Heat Crash","Heat Wave","Heavy Slam","Helping Hand","Hex","Hi Jump Kick","Hidden Power","Hidden Power Bug","Hidden Power Dark","Hidden Power Dragon","Hidden Power Electric","Hidden Power Fighting","Hidden Power Fire","Hidden Power Flying","Hidden Power Ghost","Hidden Power Grass","Hidden Power Ground","Hidden Power Ice","Hidden Power Poison","Hidden Power Psychic","Hidden Power Rock","Hidden Power Steel","Hidden Power Water","Hone Claws","Horn Attack","Horn Drill","Horn Leech","Howl","Hurricane","Hydro Cannon","Hydro Pump","Hyper Beam","Hyper Fang","Hyper Voice","Hypnosis","Ice Ball","Ice Beam","Ice Burn","Ice Fang","Ice Punch","Ice Shard","Icicle Crash","Icicle Spear","Icy Wind","Imprison","Incinerate","Inferno","Ingrain","Iron Defense","Iron Head","Iron Tail","Judgment","Jump Kick","Karate Chop","Kinesis","Knock Off","Last Resort","Lava Plume","Leaf Blade","Leaf Storm","Leaf Tornado","Leech Life","Leech Seed","Leer","Lick","Light Screen","Lock-On","Lovely Kiss","Low Kick","Low Sweep","Lucky Chant","Lunar Dance","Luster Purge","Mach Punch","Magic Coat","Magic Room","Magical Leaf","Magma Storm","Magnet Bomb","Magnet Rise","Magnitude","Me First","Mean Look","Meditate","Mega Drain","Mega Kick","Mega Punch","Megahorn","Memento","Metal Burst","Metal Claw","Metal Sound","Meteor Mash","Metronome","Milk Drink","Mimic","Mind Reader","Minimize","Miracle Eye","Mirror Coat","Mirror Move","Mirror Shot","Mist","Mist Ball","Moonlight","Morning Sun","Mud-Slap","Mud Bomb","Mud Shot","Mud Sport","Muddy Water","Nasty Plot","Natural Gift","Nature Power","Needle Arm","Night Daze","Night Shade","Night Slash","Nightmare","Octazooka","Odor Sleuth","Ominous Wind","Outrage","Overheat","Pain Split","Pay Day","Payback","Peck","Perish Song","Petal Dance","Pin Missile","Pluck","Poison Fang","Poison Gas","Poison Jab","PoisonPowder","Poison Sting","Poison Tail","Pound","Powder Snow","Power Gem","Power Split","Power Swap","Power Trick","Power Whip","Present","Protect","Psybeam","Psych Up","Psychic","Psycho Boost","Psycho Cut","Psycho Shift","Psyshock","Psystrike","Psywave","Punishment","Pursuit","Quash","Quick Attack","Quick Guard","Quiver Dance","Rage","Rage Powder","Rain Dance","Rapid Spin","Razor Leaf","Razor Shell","Razor Wind","Recover","Recycle","Reflect","Reflect Type","Refresh","Relic Song","Rest","Retaliate","Return","Revenge","Reversal","Roar","Roar of Time","Rock Blast","Rock Climb","Rock Polish","Rock Slide","Rock Smash","Rock Throw","Rock Tomb","Rock Wrecker","Role Play","Rolling Kick","Rollout","Roost","Round","Sacred Fire","Sacred Sword","Safeguard","Sand-Attack","Sand Tomb","Sandstorm","Scald","Scary Face","Scratch","Screech","Searing Shot","Secret Power","Secret Sword","Seed Bomb","Seed Flare","Seismic Toss","Selfdestruct","Shadow Ball","Shadow Claw","Shadow Force","Shadow Punch","Shadow Sneak","Sharpen","Sheer Cold","Shell Smash","Shift Gear","Shock Wave","Signal Beam","Silver Wind","Simple Beam","Sing","Sketch","Skill Swap","Skull Bash","Sky Attack","Sky Drop","Sky Uppercut","Slack Off","Slam","Slash","Sleep Powder","Sleep Talk","Sludge","Sludge Bomb","Sludge Wave","Smack Down","SmellingSalt","Smog","SmokeScreen","Snarl","Snatch","Snore","Soak","Softboiled","SolarBeam","SonicBoom","Spacial Rend","Spark","Spider Web","Spike Cannon","Spikes","Spit Up","Spite","Splash","Spore","Stealth Rock","Steel Wing","Stockpile","Stomp","Stone Edge","Stored Power","Storm Throw","Steamroller","Strength","String Shot","Struggle","Struggle Bug","Stun Spore","Submission","Substitute","Sucker Punch","Sunny Day","Super Fang","Superpower","Supersonic","Surf","Swagger","Swallow","Sweet Kiss","Sweet Scent","Swift","Switcheroo","Swords Dance","Synchronoise","Synthesis","Tackle","Tail Glow","Tail Slap","Tail Whip","Tailwind","Take Down","Taunt","Techno Blast","Teeter Dance","Telekinesis","Teleport","Thief","Thrash","Thunder","Thunder Fang","ThunderPunch","ThunderShock","Thunder Wave","Thunderbolt","Tickle","Torment","Toxic","Toxic Spikes","Transform","Tri Attack","Trick","Trick Room","Triple Kick","Trump Card","Twineedle","Twister","U-turn","Uproar","V-create","Vacuum Wave","Venoshock","ViceGrip","Vine Whip","Vital Throw","Volt Switch","Volt Tackle","Wake-Up Slap","Water Gun","Water Pledge","Water Pulse","Water Sport","Water Spout","Waterfall","Weather Ball","Whirlpool","Whirlwind","Wide Guard","Wild Charge","Will-O-Wisp","Wing Attack","Wish","Withdraw","Wonder Room","Wood Hammer","Work Up","Worry Seed","Wrap","Wring Out","X-Scissor","Yawn","Zap Cannon","Zen Headbutt"];
						MovesArray = $.grep(MovesArray, function(n, k) {
							return $.inArray(n, setmoves) === -1;
						});
						for (var z=0; z<MovesArray.length; z++) {
							movelist += '<option>' + MovesArray[z] + '</option>';
						}
						$($($('.buildpkmn')[g]).find('.move')[i-1]).html(movelist);
						movelist = '';
					}
					movefunction(tempmove, g, i);
				}
				var IVEV = $(moves[setno]).find('td');
				var IV;
				var EV;
				if (IVEV.length === 3) {
					IV = $(IVEV[1]).html();
					EV = $(IVEV[2]).html();
				}
				else {
					EV = $(IVEV[1]).html();
				}

				var StatArray = ['HP', 'Atk', 'Def', 'SpA', 'SpD', 'Spe'];
				if (EV) {
					var EVarray = EV.split('/');
					function EVset(EVarray, StatArray) {

						for (var p=0; p<EVarray.length; p++) {
							if (p === 0) { 
								$('.buildpkmn').eq(g).find('.EV').each(function() {$(this).val('0');});							
							}
							for (var q=0; q<StatArray.length; q++) {
								var Statrg = new RegExp(StatArray[q]);
								if (Statrg.test(EVarray[p]) === true) {
									$($($('.buildpkmn')[g]).find('.EV')[q]).val(parseInt(EVarray[p]));
								}
							}
						}
					}
					EVset(EVarray, StatArray);
				}
				if (IV) {
					var IVarray = IV.split('/');
					function IVset(IVarray, StatArray) {
						for (var p=0; p<IVarray.length; p++) {
							if (p === 0) { 
								$('.buildpkmn').eq(g).find('.IV').each(function() {$(this).val('31');});							
							}
							for (var q=0; q<StatArray.length; q++) {
								var Statrg = new RegExp(StatArray[q]);
								if (Statrg.test(IVarray[p]) === true) {
									$($($('.buildpkmn')[g]).find('.IV')[q]).val(parseInt(IVarray[p]));
								}
							}
						}
					}
					IVset(IVarray, StatArray);
				}
				if (!IV) {
					$($('.buildpkmn')[g]).find('.IV').each(function() {$(this).val('31');});
					setHiddenPower(g);
				}

				var tier = $($('.tier')[g]).val();
				var level = '';
				if (tier === 'Little Cup') {
					level = '5';
				}
				else if (tier === "VGC '12" || tier === "VGC '13") {
					level = '50';
				}
				else {
					level = '100';
				}
				var EVtotal = 0;
				for (var x = 0; x<6; x++) {
					EVtotal += parseInt($($($('.buildpkmn')[g]).find(".EV")[x]).val());
				}
				var EVrem = 508 - EVtotal;
				$($('.buildpkmn')[g]).find('.EVleft').html(EVrem);
				if ($($('.buildpkmn')[g]).find('.EVleft').html() < 0) {
					$($('.buildpkmn')[g]).find('.EVleft').removeClass('positive');
					$($('.buildpkmn')[g]).find('.EVleft').addClass('negative');
				}
				else {
					$($('.buildpkmn')[g]).find('.EVleft').removeClass('negative');
					$($('.buildpkmn')[g]).find('.EVleft').addClass('positive');
				}
				$($('.ability')[g]).html(abilitylist);
				$($('.item')[g]).html(itemlist);
				$($('.nature')[g]).html(naturelist);
				$($('.level')[g]).val(level);
				setNature(g);
			});
		}
		function exportteam() {
			var exporttext = '';
			var StatArray = ['HP', 'Atk', 'Def', 'SAtk', 'SDef', 'Spd'];
			if ($('input[name=sim]:checked').val() === 'PS') {
				var isShowdown = true;
			}
			else {
				var isShowdown = false;
			}
			for (var i=0; i<6; i++) {
				var PokemonName =  $($('.buildpkmn')[i]).find('.pkmn').val();
				var moves = [];
				for (var u=0; u<4; u++) {			
					if ($('.buildpkmn').eq(i).find('.move option:selected').eq(u).val()) {
						moves.push($($($('.buildpkmn')[i]).find('.move')[u]).val());
					}
				}
				if (PokemonName && $($('.buildpkmn')[i]).find('.ability').val() && moves.length > 0) {
				var item = $($('.buildpkmn')[i]).find('.item').val();
				var ability = $($('.buildpkmn')[i]).find('.ability').val();
				var nature = $($('.buildpkmn')[i]).find('.nature').val();
				var EVArray = [];
				for (var c=0; c<6; c++) {
					EVArray.push(parseInt($($($('.buildpkmn')[i]).find('.EV')[c]).val()));			
				}
				var EVSpread = '';
				for (var j=0; j<6; j++) {
					if (EVArray[j] !== 0) {
					EVSpread += EVArray[j] + ' ' + StatArray[j] + ' / ';
					}
				}
				EVSpread = EVSpread.substr(0,EVSpread.length-3);
				var IVArray = [];
				for (var q=0; q<6; q++) {
					IVArray.push(parseInt($($($('.buildpkmn')[i]).find('.IV')[q]).val()));			
				}
				//exemption for Pokemon Online export
				if (isShowdown) {
					var IVSpread = '';
					for (var e=0; e<6; e++) {
						if (IVArray[e] !== 31) {
						IVSpread += IVArray[e] + ' ' + StatArray[e] + ' / ';
						}
					}
					if (IVSpread.length > 5) {
						IVSpread = IVSpread.substr(0,IVSpread.length-3);
					}
					var level = $($('.level')[i]).val();
				}
				exporttext += PokemonName + ' @ ' + item +'\n';
				exporttext += 'Trait: ' + ability +'\n';
				if (isShowdown && level !== '100') {
					exporttext += 'Level: ' + level +'\n';
				}
				if (EVSpread.length > 0) {
					exporttext += 'EVs: ' + EVSpread + '\n';
				}
				if (isShowdown && IVSpread.length > 0 ) {
					exporttext += 'IVs: ' + IVSpread + '\n';
				}
				exporttext += nature + ' Nature\n';
				for (var n=0; n<moves.length; n++) {
					if (moves[n].substr(0,13) === 'Hidden Power ' && !isShowdown) {
						exporttext += '- Hidden Power ' + '[' + moves[n].substr(13) + ']\n';
					}
					else {
						exporttext += '- ' + moves[n] + '\n';
					}
				}
				exporttext += '\n';
			}
			}
			$('#export').text(exporttext);
		}
		//]]>
	</script>
